<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IServiceDonHang" />
        <binding name="BasicHttpBinding_IServiceKhachHang" />
        <binding name="BasicHttpBinding_IServiceSanPham" />
        <binding name="BasicHttpBinding_IServiceNhanVien" />
        <binding name="BasicHttpBinding_IServiceNhaSanXuat" />
        <binding name="BasicHttpBinding_IServiceQuyenNhanVien" />
        <binding name="BasicHttpBinding_IServicePhieuNhap" />
        <binding name="BasicHttpBinding_IServiceNhaPhanPhoi" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:3015/ServiceDonHang.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceDonHang"
        contract="ServiceDonHang.IServiceDonHang" name="BasicHttpBinding_IServiceDonHang" />
      <endpoint address="http://localhost:3015/ServiceKhachHang.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceKhachHang"
        contract="ServiceKhachHang.IServiceKhachHang" name="BasicHttpBinding_IServiceKhachHang" />
      <endpoint address="http://localhost:3015/ServiceSanPham.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceSanPham"
        contract="ServiceSanPham.IServiceSanPham" name="BasicHttpBinding_IServiceSanPham" />
      <endpoint address="http://localhost:3015/ServiceNhanVien.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceNhanVien"
        contract="ServiceNhanVien.IServiceNhanVien" name="BasicHttpBinding_IServiceNhanVien" />
      <endpoint address="http://localhost:3015/ServiceNhaSanXuat.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceNhaSanXuat"
        contract="ServiceNhaSanXuat.IServiceNhaSanXuat" name="BasicHttpBinding_IServiceNhaSanXuat" />
      <endpoint address="http://localhost:3015/ServiceQuyenNhanVien.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceQuyenNhanVien"
        contract="ServiceQuyenNhanVien.IServiceQuyenNhanVien" name="BasicHttpBinding_IServiceQuyenNhanVien" />
      <endpoint address="http://localhost:3015/ServicePhieuNhap.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServicePhieuNhap"
        contract="ServicePhieuNhap.IServicePhieuNhap" name="BasicHttpBinding_IServicePhieuNhap" />
      <endpoint address="http://localhost:3015/ServiceNhaPhanPhoi.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServiceNhaPhanPhoi"
        contract="ServiceNhaPhanPhoi.IServiceNhaPhanPhoi" name="BasicHttpBinding_IServiceNhaPhanPhoi" />
    </client>
  </system.serviceModel>
</configuration>